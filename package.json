        "@graphql-tools/mock": "^8.0.0",
        "@types/swiper": "^5.4.2",
        "apollo": "^2.31.1",
        "apollo-angular": "^2.3.0",
        "apollo-upload-client": "^14.1.3",
        "bugsnag-build-reporter": "^1.0.3",
        "dotenv": "^8.2.0",
        "graphql": "^15.5.0",
